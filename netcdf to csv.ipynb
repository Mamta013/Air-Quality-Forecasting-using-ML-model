        "import xarray as xr\n",
        "from google.colab import files\n",
        "\n",
        "DS = xr.open_dataset(\"/content/vertical wind.nc\")\n",
        "\n",
        "csv_filename = \"vertical wind.csv\"\n",
        "DS.to_dataframe().to_csv(csv_filename)\n",
        "\n",
        "files.download(csv_filename)\n"
